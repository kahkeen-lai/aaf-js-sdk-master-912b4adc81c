{
  "name": "@xaaf/root",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "all": "yarn --cache-folder ../.cache/yarn && run-s bootstrap build analyze test:all docs docs:internal",
    "clean": "rimraf ./packages/*/*/node_modules ./packages/*/*/dist ./packages/*/*/lib ./packages/*/*/.jest ./packages/*/*/yarn.lock ./packages/*/*/coverage",
    "clean:jest": "rimraf ./packages/*/*/.jest ./packages/*/*/coverage",
    "build": "lerna run build",
    "bundle": "lerna run bundle",
    "analyze": "lerna run analyze",
    "test": "make-dir coverage && lerna run test && yarn coverage",
    "test:all": "run-p test test:integration && yarn coverage",
    "test:integration": "lerna run test:integration",
    "test:integration:custom_api_key": "lerna run test:custom_api_key:integration:general",
    "coverage": "node ./coverage.js",
    "bootstrap": "lerna bootstrap && lerna link",
    "bootstrap:ci": "lerna bootstrap -- --chromedriver-skip-install --cache-folder /users/m32636/.cache/yarn&& lerna link",
    "bootstrap:dev": "lerna bootstrap --force-local && yarn build",
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "changelog-debug": "auto-changelog --template changelog-template.hbs -p --template json --output changelog-data.json",
    "reset": "yarn clean && yarn bootstrap",
    "watch": "lerna run watch --parallel",
    "buildConfig": "node ./run-command.js",
    "lint": "lerna run lint -- --quiet ",
    "lint:fix": "lerna run lint:fix",
    "docs": "lerna run docs",
    "ci:get-changed-files": "git diff --name-status $HEAD origin/master | grep '^A|^M' -iE | tr '^A' ' ' | tr '^M' ' ' ",
    "ci:lint-changed": "eslint $(yarn ci:get-changed-files | grep 'js$|tsx$|ts$' -iE | tr '\\n' ' ') --quiet",
    "ci:docs": "yarn docs",
    "ci:build": "yarn --cache-folder /users/m32636/.cache/yarn && run-s bootstrap:ci build ci:lint-changed analyze",
    "ci:prod": "rimraf ./packages/*/*/node_modules && lerna bootstrap -- --production",
    "ci:test": "yarn test",
    "docs:internal": "docsify serve ./docs/internal",
    "docs:public": "docsify serve ./docs/public",
    "run-web": "cd packages/demos/demo-web && yarn start",
    "run-android": "cd packages/demos/demo-youitv && yarn run-android",
    "mutation": "lerna run mutation",
    "run:native:android": "lerna run android --scope=\"xaaf-tvos-demo\"",
    "run:native:start": "lerna run start --scope=\"xaaf-tvos-demo\"",
    "build:android:release": "lerna run build:android:release --scope=\"xaaf-tvos-demo\"",
    "run:setup:ios": "lerna run setup:ios --scope=\"xaaf-tvos-demo\"",
    "run:native:ios": "yarn run:native:start & && lerna run ios --scope=\"xaaf-tvos-demo\"",
    "run:tvos": "lerna run ios:release --scope=\"xaaf-tvos-demo\"",
    "build:xaaf-js-sdk": "lerna run build --scope=\"@xaaf/xaaf-js-sdk\"",
    "build:aaf-rn-sdk": "lerna run build --scope=\"@xaaf/aaf-rn-sdk\"",
    "version": "lerna version",
    "publish": "lerna publish from-package"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 2
  },
  "husky": {
    "hooks": {
      "pre-commitx": "lint-staged",
      "post-commitx": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
